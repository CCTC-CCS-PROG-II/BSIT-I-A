public class URDANETA_ACTIVITY_I{
    public static void main (String[] args){
        int a = 2;
        int z = 4;
        int v = 6;
        System.out.println("Sum = " +  (a+z+v));
        System.out.println("Difference = " + (a-z-v));
        System.out.println("Product = " + (a*z*v));
        System.out.println("Quotient = " + (a/z/v));
    }
}
